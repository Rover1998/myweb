<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%date %-5level %c %msg %ex{full}%n " />
		</Console>
	</Appenders>
	<Loggers>

		<root level="INFO">
			<AppenderRef ref="Console" />
		</root>
	</Loggers>
</Configuration>